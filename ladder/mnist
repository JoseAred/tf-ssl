#!/usr/bin/env bash

source activate blaze

nohup python main.py --id 'gamma' --gamma --train_flag --decay_start_epoch 100 --end_epoch 150 --print_interval 50 --save_epochs 1 > logs/gamma.res &


python test.py --id 'gamma' --gamma --train_step 876

nohup python main.py --id 'ladder_test' --train_flag --decay_start_epoch 100 --end_epoch 150 --print_interval 50 --save_epochs 10 --write_to logs/ladder_test.res &
